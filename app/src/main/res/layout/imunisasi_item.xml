<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/nama_imunisasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="Jenis Imunisasi"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gambarStatus"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginEnd="30dp"
        android:src="@drawable/belum_imunisasi"
        app:layout_constraintBottom_toBottomOf="@+id/nama_imunisasi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/nama_imunisasi" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/baseline_chevron_right_24"
        app:layout_constraintStart_toEndOf="@id/gambarStatus"
        app:layout_constraintTop_toTopOf="@id/gambarStatus"/>


</androidx.constraintlayout.widget.ConstraintLayout>